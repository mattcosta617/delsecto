<body>
    <%- include ('../partials/header.ejs'); -%>
    <main>  
      <h1 class="log questionsTop" id="topLog">Login</h1>
      <div class="log">
        <form method="POST">
            <div>
              <input class="loginput" type="text" name="username" placeholder="email">
            </div>
            <div>
              <input class="loginput" type="text" name="password" placeholder="password"></input>
            </div>
            <button class="button" id="logButton" type="submit">Login</button>
          </form>
         <button class="button"> <a class="signup" href="/users/register">Signup!</a> </button>
      </div>
    </main>

    <div id="bug">
      <div id="leftAntenna"></div>
      <div id="rightAntenna"></div>
      <div id="bugHead"></div>
      <div id="bugBody"></div>
      <div id="bugLeftArm"></div>
      <div id="bugRightArm"></div>
      <div id="bugMiddleLeft"></div>
      <div id="bugMiddleRight"></div>
      <div id="bugLeftLeg"></div>
      <div id="bugRightLeg"></div>
      <div id="leftWing"></div>
      <div id="rightWing"></div>
      </div>
</body>
</html>